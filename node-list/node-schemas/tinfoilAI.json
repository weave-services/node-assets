      "options": [
        {
          "label": "Chat Completion",
          "name": "chatCompletion"
        }
      ]
    }
  ],
  "credentials": [
    {
      "label": "Credential Method",
      "name": "credentialMethod",
      "type": "string",
      "options": [
        {
          "label": "Tinfoil API Key",
          "name": "tinfoilApi"
        }
      ],
      "default": "tinfoilApi"
    }
  ],
  "inputParameters": [
    {
      "label": "Model",
      "name": "model",
      "type": "options",
      "options": [
        {
          "label": "DeepSeek-R1-Distill-Llama-70B",
          "name": "deepseek-r1-70b"
        },
        {
          "label": "Mistral-Small-3.1-24B",
          "name": "mistral-small-3.1-24b"
        },
        {
          "label": "Llama 3.3 70B",
          "name": "llama-3.3-70b"
        },
        {
          "label": "Llama 3.2 1B",
          "name": "llama-3.2-1b"
        },
        {
          "label": "Llama Guard 3 1B",
          "name": "llama-guard-3-1b"
        },
        {
          "label": "Qwen 2.5 Coder 0.5B",
          "name": "qwen-2.5-coder-0.5b"
        },
        {
          "label": "Nomic Embed Text",
          "name": "nomic-embed-text"
        }
      ],
      "description": "Which Tinfoil model to use for the chat completion.",
      "default": "deepseek-r1-70b",
      "show": {
        "actions.operation": [
          "chatCompletion"
        ]
      }
    },
    {
      "label": "Enclave",
      "name": "enclave",
      "type": "string",
      "description": "The Tinfoil enclave host for your model.",
