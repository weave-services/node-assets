          "listActivities",
          "listChannels",
          "listChannelSections",
          "insertChannelBanner",
          "listCommentThreads",
          "insertCommentThread",
          "listPlaylistItems",
          "listPlaylists",
          "listSubscriptions",
          "insertSubscription",
          "setWatermark",
          "unsetWatermark"
        ]
      }
    },
    {
      "label": "Mine",
      "name": "mine",
      "type": "boolean",
      "description": "If true, use the authenticated user's channel",
      "optional": true,
      "default": false,
      "show": {
        "actions.api": [
          "listActivities",
          "listChannels",
          "listChannelSections",
          "listPlaylists",
          "listSubscriptions"
        ]
      }
    },
    {
      "label": "Include Home Activities (Deprecated)",
      "name": "home",
      "type": "boolean",
      "description": "If true, retrieve home feed activities (deprecated)",
      "optional": true,
      "default": false,
      "show": {
        "actions.api": [
          "listActivities"
        ]
      }
    },
    {
      "label": "Published After",
      "name": "publishedAfter",
      "type": "string",
      "description": "ISO 8601 dateTime to filter activities published after this time",
      "optional": true,
      "show": {
        "actions.api": [
          "listActivities"
        ]
      }
    },
    {
      "label": "Published Before",
      "name": "publishedBefore",
      "type": "string",
      "description": "ISO 8601 dateTime to filter activities published before this time",
      "optional": true,
      "show": {
        "actions.api": [
          "listActivities"
        ]
      }
    },
    {
      "label": "Region Code",
      "name": "regionCode",
      "type": "string",
      "description": "ISO country code",
      "optional": true,
      "show": {
        "actions.api": [
          "listActivities",
          "listGuideCategories",
          "listVideoCategories",
          "search",
          "listVideos"
        ]
      }
    },
    {
      "label": "Description",
      "name": "description",
      "type": "string",
      "description": "Bulletin text to post",
      "show": {
        "actions.api": [
          "insertActivity",
          "insertPlaylist",
          "updatePlaylist",
          "insertVideo",
          "updateVideo"
        ]
      }
    },
    {
      "label": "Resource ID",
      "name": "resourceId",
      "type": "string",
      "description": "ID of the resource (video or playlist) to attach in the bulletin",
      "optional": true,
      "show": {
        "actions.api": [
          "insertActivity"
        ]
      }
    },
    {
      "label": "Video ID",
      "name": "videoId",
      "type": "string",
      "description": "Video ID",
      "optional": true,
      "show": {
        "actions.api": [
          "listCaptions",
          "listPlaylistItems",
          "insertPlaylistItem",
          "listVideos",
          "insertVideo",
          "updateVideo",
          "rateVideo",
          "getVideoRating",
          "reportVideoAbuse",
          "deleteVideo"
        ]
      }
    },
    {
      "label": "Caption ID",
      "name": "captionId",
      "type": "string",
      "description": "Caption track ID",
      "optional": true,
      "show": {
        "actions.api": [
          "listCaptions",
          "downloadCaption",
          "deleteCaption"
        ]
      }
    },
    {
      "label": "Language",
      "name": "language",
      "type": "string",
      "description": "Caption language (ISO 639-1 code)",
      "show": {
        "actions.api": [
          "insertCaption",
          "reportVideoAbuse"
        ]
      }
    },
    {
      "label": "Caption Name",
      "name": "captionName",
      "type": "string",
      "description": "Name of the caption track",
      "show": {
        "actions.api": [
          "insertCaption"
        ]
      }
    },
    {
      "label": "Is Draft",
      "name": "isDraft",
      "type": "boolean",
      "description": "Whether to mark the caption track as draft (unpublished)",
      "optional": true,
      "default": false,
      "show": {
        "actions.api": [
          "insertCaption",
          "updateCaption"
        ]
      }
    },
    {
      "label": "Synchronize Timings",
      "name": "sync",
      "type": "boolean",
      "description": "Auto-synchronize captions with the video track (deprecated)",
      "optional": true,
      "default": false,
      "show": {
        "actions.api": [
          "insertCaption",
          "updateCaption"
        ]
      }
    },
    {
      "label": "Caption File Path",
      "name": "captionFile",
      "type": "string",
      "description": "Path or URL of the caption file",
      "optional": true,
      "show": {
        "actions.api": [
          "insertCaption",
          "updateCaption"
        ]
      }
    },
    {
      "label": "Secondary Reason ID",
      "name": "secondaryReasonId",
      "type": "string",
      "description": "Secondary reason ID (if applicable)",
      "optional": true,
      "show": {
        "actions.api": [
          "reportVideoAbuse"
        ]
      }
    },
    {
      "label": "Parent ID",
      "name": "parentId",
      "type": "string",
      "description": "ID of the comment for which to retrieve replies",
      "optional": true,
      "show": {
        "actions.api": [
          "listComments",
          "insertComment"
        ]
      }
    },
    {
      "label": "Text Prompt",
      "name": "prompt",
